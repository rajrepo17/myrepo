pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                echo 'checkout'
            }
        }
         stage('build') {
            steps {
                echo 'build'
            }
        }

        stage('build') {
            steps {
                echo 'build'
            }
        }
         stage('release') {
            steps {
                echo 'release'
            }
        }
    }
}
